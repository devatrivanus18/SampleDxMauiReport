@page "/report1"
@inject IJSRuntime JSRuntime
@implements IDisposable
<div id="report1" style="width:100%; height: 800px" data-bind="dxReportViewer: $data"></div>

@code {
    protected override void OnAfterRender(bool firstRender)
    {
        JSRuntime.InvokeAsync<object>("JsFunctions.InitWebReport1Viewer");
    }

    public void Dispose()
    {
        JSRuntime.InvokeAsync<string>("JsFunctions.Dispose", "report1");
    }
}
