@page "/report1"
@inject IJSRuntime JSRuntime
@implements IDisposable
<div id="report1" style="width:100%; height: 90vh;" data-bind="dxReportViewer: $data"></div>

@code {
    protected override void OnAfterRender(bool firstRender)
    {
        JSRuntime.InvokeAsync<string>("JsFunctions.InitWebDocumentViewer", "report1", "Report1");
    }

    public void Dispose()
    {
        JSRuntime.InvokeAsync<string>("JsFunctions.Dispose", "report1");
    }
}
